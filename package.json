{
  "name": "mcp-server-connection",
  "version": "1.0.0",
  "description": "A TypeScript MCP (Model Context Protocol) server implementation",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:profile": "node dist/profile-server.js",
    "start:standalone": "node dist/standalone-profile-server.js",
    "start:enhanced": "node dist/enhanced-profile-server.js",
    "dev": "tsc --watch & node --watch dist/index.js",
    "dev:profile": "tsc --watch & node --watch dist/profile-server.js",
    "dev:standalone": "tsc --watch & node --watch dist/standalone-profile-server.js",
    "dev:enhanced": "tsc --watch & node --watch dist/enhanced-profile-server.js",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "render-build": "npm install && npm run build",
    "render-start": "npm run start:standalone"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "typescript",
    "server"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "aws-sdk": "^2.1691.0",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "typescript": "^5.5.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
